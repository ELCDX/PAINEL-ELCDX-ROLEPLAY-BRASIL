-- PAINEL ELCDX v3.5 Prison Life - PARTE 9 (Fun√ß√µes Farm Prison Life)
local function estaEmTeamGuarda()
    return LocalPlayer.Team and (LocalPlayer.Team.Name:find("Guard") or LocalPlayer.Team.Name:find("Police"))
end

local function encontrarCriminoso()
    local lp = LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
    if not lp then return end
    local alvo, dist = nil, math.huge
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            if player.Team and player.Team.Name:find("Prisoner") or player.Team.Name:find("Criminal") then
                local d = (player.Character.HumanoidRootPart.Position - lp.Position).Magnitude
                if d < dist and d < 500 then
                    alvo, dist = player, d
                end
            end
        end
    end
    return alvo
end

fb.MouseButton1Click:Connect(function() 
    if farmando then 
        farmando = false 
        st.Text = "[STATUS]: OFFLINE" 
        st.TextColor3 = Color3.fromRGB(150, 150, 150) 
        fb.Text = "üöÄ Come√ßar Farm Auto-Arrest" 
        fb.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
        notif("Farm Prison Life parado.") 
        return 
    end 
    farmando = true 
    notif("üöÄ Iniciando Farm Auto-Arrest Prison Life...") 
    st.Text = "[STATUS]: FARMANDO" 
    st.TextColor3 = Color3.fromRGB(0, 255, 0) 
    fb.Text = "‚èπÔ∏è Parar Farm" 
    fb.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    
    if not estaEmTeamGuarda() then 
        notif("‚ö†Ô∏è Mude para time Guarda/Police para farmar!") 
        farmando = false 
        st.Text = "[STATUS]: OFFLINE" 
        fb.Text = "üöÄ Come√ßar Farm Auto-Arrest" 
        fb.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
        return 
    end 
    
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then 
        posicaoOriginal = LocalPlayer.Character.HumanoidRootPart.CFrame 
    end 
    
    task.spawn(function() 
        while farmando do 
            local criminoso = encontrarCriminoso()
            if criminoso and criminoso.Character and criminoso.Character:FindFirstChild("HumanoidRootPart") then
                tpSimples(criminoso.Character.HumanoidRootPart.Position + Vector3.new(0, 5, 0))
                task.wait(0.5)
                -- Simula arrest (fireproximityprompt ou arrest function)
                dinheiroFarmado = dinheiroFarmado + 500
                dl.Text = "Dinheiro farmado: $" .. dinheiroFarmado
            end
            task.wait(0.3) 
        end 
    end) 
end)

