espBox.Visible = false
espBox.Color = Color3.fromRGB(0, 255, 0)
espBox.Thickness = 1
espBox.Filled = false
espBox.Transparency = 1

-- ATUALIZAR FPS/PING
local lastUpdate = tick()
local fps = 0
spawn(function()
    while wait(0.5) do
        if tick() - lastUpdate >= 1 then
            fps = math.floor(1 / RunService.RenderStepped:Wait())
            lastUpdate = tick()
        end
        fpsLabel.Text = "FPS: " .. fps
        
        local ping = math.floor(Stats.Network.ServerStatsItem["Data Ping"]:GetValue())
        pingLabel.Text = "PING: " .. ping .. "ms"
        
        if ping < 100 then
            pingLabel.TextColor3 = Color3.fromRGB(0, 255, 0)
        elseif ping < 200 then
            pingLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
        else
            pingLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
        end
    end
end)

-- MINIMIZAR
local function toggleMinimize()
    GUI_MINIMIZED = not GUI_MINIMIZED
    
    if GUI_MINIMIZED then
        mainFrame.Size = UDim2.new(0, 160, 0, 28)
        scrollFrame.Visible = false
        statsFrame.Visible = false
        minimizeButton.Text = "+"
    else
        mainFrame.Size = UDim2.new(0, 160, 0, 280)
        scrollFrame.Visible = true
        statsFrame.Visible = true
        minimizeButton.Text = "-"
    end
end

-- FECHAR
local noclipConnection
local infiniteJumpConnection
local function closeGUI()
    AIM_ASSIST_ON = false
    NOCLIP_ON = false
    SPEED_ON = false
    INFINITE_JUMP_ON = false
    
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.WalkSpeed = WALK_SPEED
    end
    
    if noclipConnection then
        noclipConnection:Disconnect()
    end
    
    if infiniteJumpConnection then
        infiniteJumpConnection:Disconnect()
    end
    
    tracerLine:Remove()
    espBox:Remove()
    screenGui:Destroy()
end

minimizeButton.MouseButton1Click:Connect(toggleMinimize)
closeButton.MouseButton1Click:Connect(closeGUI)

-- VERIFICAR TEAM
local function CheckTeam(player)
    if not (TARGET_PRESOS or TARGET_GUARDAS or TARGET_CRIMINOSOS) then
        return false
    end
    
    if player.Team then
        local teamName = player.Team.Name
        local teamColor = player.Team.TeamColor.Name
        
        if TARGET_PRESOS and (teamName:lower():find("prisoner") or teamColor == "Bright orange") then
            return true
        end
        if TARGET_GUARDAS and (teamName:lower():find("guard") or teamColor == "Bright blue") then
            return true
        end
        if TARGET_CRIMINOSOS and (teamName:lower():find("criminal") or teamColor == "Really red") then
            return true
        end
    end
    
    return false
end

-- ENCONTRAR JOGADOR MAIS PRÓXIMO
function GetClosestPlayer()
    local MaxDist = FOV
    local ClosestPlayer = nil
    local ScreenCenter = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
    
    for _, Player in pairs(Players:GetPlayers()) do
        if Player ~= LocalPlayer and Player.Character and CheckTeam(Player) then
            local Humanoid = Player.Character:FindFirstChild("Humanoid")
            local Head = Player.Character:FindFirstChild("Head")
            
            if Humanoid and Humanoid.Health > 0 and Head then
                local HeadPos, OnScreen = Camera:WorldToViewportPoint(Head.Position)
                
                if OnScreen then
                    local HeadScreenPos = Vector2.new(HeadPos.X, HeadPos.Y)
                    local Distance = (HeadScreenPos - ScreenCenter).Magnitude
                    
                    if Distance < MaxDist then
                        MaxDist = Distance
                        ClosestPlayer = Player.Character
                    end
                end
            end
        end
    end
    
    return ClosestPlayer
end

-- NOCLIP
local function toggleNoclip()
    NOCLIP_ON = not NOCLIP_ON
    
    if NOCLIP_ON then
        noclipButton.Text = "NOCLIP: ON"
        noclipButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
        
        noclipConnection = RunService.Stepped:Connect(function()
            if LocalPlayer.Character then
                for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
    else
        noclipButton.Text = "NOCLIP: OFF"
        noclipButton.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        
        if noclipConnection then
            noclipConnection:Disconnect()
        end
    end
end

-- SPEED
local function toggleSpeed()
    SPEED_ON = not SPEED_ON
    
    if SPEED_ON then
        speedButton.Text = "SPEED: ON"
        speedButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = SPRINT_SPEED
        end
    else
        speedButton.Text = "SPEED: OFF"
        speedButton.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = WALK_SPEED
        end
    end
end

-- INFINITE JUMP
local function toggleInfiniteJump()
    INFINITE_JUMP_ON = not INFINITE_JUMP_ON
    
    if INFINITE_JUMP_ON then
        infiniteJumpButton.Text = "INF JUMP: ON"
        infiniteJumpButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
        
        infiniteJumpConnection = UserInputService.JumpRequest:Connect(function()
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
    else
        infiniteJumpButton.Text = "INF JUMP: OFF"
        infiniteJumpButton.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
        
        if infiniteJumpConnection then
            infiniteJumpConnection:Disconnect()
        end
    end
end

