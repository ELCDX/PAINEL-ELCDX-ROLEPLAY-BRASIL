noclipButton.MouseButton1Click:Connect(toggleNoclip)
speedButton.MouseButton1Click:Connect(toggleSpeed)
infiniteJumpButton.MouseButton1Click:Connect(toggleInfiniteJump)

-- LOOP AIMBOT
RunService.RenderStepped:Connect(function()
    if AIM_ASSIST_ON and UserInputService:IsKeyDown(Enum.KeyCode.E) then
        local closestChar = GetClosestPlayer()
        if closestChar and closestChar:FindFirstChild("Head") and closestChar:FindFirstChild("HumanoidRootPart") then
            local head = closestChar.Head
            local hrp = closestChar.HumanoidRootPart
            
            local velocity = hrp.Velocity
            local targetPos = head.Position + (velocity * 0.1)
            
            local camCFrame = Camera.CFrame
            local newCFrame = CFrame.new(camCFrame.Position, targetPos)
            Camera.CFrame = camCFrame:Lerp(newCFrame, SMOOTHNESS)
            
            local headScreenPos, onScreen = Camera:WorldToViewportPoint(head.Position)
            if onScreen then
                local screenCenter = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
                tracerLine.From = screenCenter
                tracerLine.To = Vector2.new(headScreenPos.X, headScreenPos.Y)
                tracerLine.Visible = true
                
                local rootPos = Camera:WorldToViewportPoint(hrp.Position)
                local headPos = Camera:WorldToViewportPoint(head.Position + Vector3.new(0, 1, 0))
                
                if rootPos and headPos then
                    local height = math.abs(headPos.Y - rootPos.Y)
                    local width = height / 2
                    
                    espBox.Size = Vector2.new(width, height)
                    espBox.Position = Vector2.new(rootPos.X - width/2, rootPos.Y - height/2)
                    espBox.Visible = true
                end
            else
                tracerLine.Visible = false
                espBox.Visible = false
            end
        else
            tracerLine.Visible = false
            espBox.Visible = false
        end
    else
        tracerLine.Visible = false
        espBox.Visible = false
    end
end)

