    TARGET_GUARDAS = not TARGET_GUARDAS
    if TARGET_GUARDAS then
        guardasButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
    else
        guardasButton.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    end
end)

criminososButton.MouseButton1Click:Connect(function()
    TARGET_CRIMINOSOS = not TARGET_CRIMINOSOS
    if TARGET_CRIMINOSOS then
        criminososButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
    else
        criminososButton.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    end
