-- PAINEL ELCDX v1.2 - PARTE 3 (FPS, MINIMIZAR, BOTOES, AIMBOT CLÁSSICO)

local lastUpdate = tick()
local fps = 0
spawn(function()
    while task.wait(0.5) do
        if tick() - lastUpdate >= 1 then
            fps = math.floor(1 / RunService.RenderStepped:Wait())
            lastUpdate = tick()
        end
        fpsLabel.Text = "FPS: " .. fps
    end
end)

local noclipConnection
local infiniteJumpConnection

local function toggleMinimize()
    GUI_MINIMIZED = not GUI_MINIMIZED
    if GUI_MINIMIZED then
        mainFrame.Size = UDim2.new(0, 160, 0, 28)
        scrollFrame.Visible = false
        statsFrame.Visible = false
        minimizeButton.Text = "+"
    else
        mainFrame.Size = UDim2.new(0, 160, 0, 280)
        scrollFrame.Visible = true
        statsFrame.Visible = true
        minimizeButton.Text = "-"
    end
end

local function closeGUI()
    AIMBOT_ON = false
    NOCLIP_ON = false
    SPEED_ON = false
    INFINITE_JUMP_ON = false

    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.WalkSpeed = WALK_SPEED
    end

    if noclipConnection then
        noclipConnection:Disconnect()
    end

    if infiniteJumpConnection then
        infiniteJumpConnection:Disconnect()
    end

    screenGui:Destroy()
end

minimizeButton.MouseButton1Click:Connect(toggleMinimize)
closeButton.MouseButton1Click:Connect(closeGUI)

aimbotExpand.MouseButton1Click:Connect(function()
    teamsFrame.Visible = not teamsFrame.Visible
    aimbotExpand.Text = teamsFrame.Visible and "-" or "+"
end)

presosButton.MouseButton1Click:Connect(function()
    TARGET_PRESOS = not TARGET_PRESOS
    presosButton.BackgroundColor3 =
        TARGET_PRESOS and Color3.fromRGB(0,170,0) or Color3.fromRGB(45,45,45)
end)

guardasButton.MouseButton1Click:Connect(function()
    TARGET_GUARDAS = not TARGET_GUARDAS
    guardasButton.BackgroundColor3 =
        TARGET_GUARDAS and Color3.fromRGB(0,170,0) or Color3.fromRGB(45,45,45)
end)

criminososButton.MouseButton1Click:Connect(function()
    TARGET_CRIMINOSOS = not TARGET_CRIMINOSOS
    criminososButton.BackgroundColor3 =
        TARGET_CRIMINOSOS and Color3.fromRGB(0,170,0) or Color3.fromRGB(45,45,45)
end)

noclipButton.MouseButton1Click:Connect(function()
    NOCLIP_ON = not NOCLIP_ON
    if NOCLIP_ON then
        noclipButton.Text = "NOCLIP: ON"
        noclipButton.BackgroundColor3 = Color3.fromRGB(0,170,0)
        noclipConnection = RunService.Stepped:Connect(function()
            if LocalPlayer.Character then
                for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
    else
        noclipButton.Text = "NOCLIP: OFF"
        noclipButton.BackgroundColor3 = Color3.fromRGB(45,45,45)
        if noclipConnection then
            noclipConnection:Disconnect()
        end
    end
end)

speedButton.MouseButton1Click:Connect(function()
    SPEED_ON = not SPEED_ON
    if SPEED_ON then
        speedButton.Text = "SPEED: ON"
        speedButton.BackgroundColor3 = Color3.fromRGB(0,170,0)
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = SPRINT_SPEED
        end
    else
        speedButton.Text = "SPEED: OFF"
        speedButton.BackgroundColor3 = Color3.fromRGB(45,45,45)
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = WALK_SPEED
        end
    end
end)

infiniteJumpButton.MouseButton1Click:Connect(function()
    INFINITE_JUMP_ON = not INFINITE_JUMP_ON
    if INFINITE_JUMP_ON then
        infiniteJumpButton.Text = "INF JUMP: ON"
        infiniteJumpButton.BackgroundColor3 = Color3.fromRGB(0,170,0)
        infiniteJumpConnection = UserInputService.JumpRequest:Connect(function()
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
    else
        infiniteJumpButton.Text = "INF JUMP: OFF"
        infiniteJumpButton.BackgroundColor3 = Color3.fromRGB(45,45,45)
        if infiniteJumpConnection then
            infiniteJumpConnection:Disconnect()
        end
    end
end)

-- AIMBOT PRISON LIFE ORIGINAL CONTROLADO PELO BOTÃO

local GetPlayers = Players.GetPlayers
local WTSP = Camera.WorldToScreenPoint
local FindFirstChild = game.FindFirstChild
local Vector2_new = Vector2.new
local Mouse = LocalPlayer:GetMouse()

local function ClosestChar()
    local Max, Close = math.huge
    for _,V in pairs(GetPlayers(Players)) do
        if V ~= LocalPlayer and V.Team ~= LocalPlayer.Team and V.Character then
            local Torso = FindFirstChild(V.Character, "Torso")
            if Torso then
                local Pos, OnScreen = WTSP(Camera, Torso.Position)
                if OnScreen then
                    local Dist = (Vector2_new(Pos.X, Pos.Y) - Vector2_new(Mouse.X, Mouse.Y)).Magnitude
                    if Dist < Max then
                        Max = Dist
                        Close = V.Character
                    end
                end
            end
        end
    end
    return Close
end

local MT = getrawmetatable(game)
local __namecall = MT.__namecall
setreadonly(MT, false)

local aimbotHookEnabled = false

local function enableHook()
    if aimbotHookEnabled then return end
    aimbotHookEnabled = true
    MT.__namecall = newcclosure(function(self, ...)
        local Method = getnamecallmethod()
        if Method == "FindPartOnRay" and not checkcaller() and tostring(getfenv(0).script) == "GunInterface" and AIMBOT_ON then
            local Character = ClosestChar()
            if Character and Character:FindFirstChild("Torso") then
                return Character.Torso, Character.Torso.Position
            end
        end
        return __namecall(self, ...)
    end)
end

enableHook()
setreadonly(MT, true)

aimbotButton.MouseButton1Click:Connect(function()
    AIMBOT_ON = not AIMBOT_ON
    if AIMBOT_ON then
        aimbotButton.Text = "AIMBOT: ON"
        aimbotButton.BackgroundColor3 = Color3.fromRGB(0,170,0)
    else
        aimbotButton.Text = "AIMBOT: OFF"
        aimbotButton.BackgroundColor3 = Color3.fromRGB(45,45,45)
    end
end)

-- PAINEL ELCDX v1.2 - PARTE 4 (ANTI-AFK)

local vu = game:GetService("VirtualUser")
LocalPlayer.Idled:Connect(function()
    vu:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
    task.wait(1)
    vu:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
end)
