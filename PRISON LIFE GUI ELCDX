-- PAINEL ELCDX v3.5 Prison Life - PARTE 11 (Fun√ß√µes P√°gina 3)
be.MouseButton1Click:Connect(function() 
    espAtivo = not espAtivo 
    be.Text = espAtivo and "üëÅÔ∏è ESP (Ativado)" or "üëÅÔ∏è ESP (Desativado)" 
    be.BackgroundColor3 = espAtivo and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(50, 50, 50)
    for _, p in pairs(Players:GetPlayers()) do 
        if p.Character and p ~= LocalPlayer then 
            local ex = p.Character:FindFirstChild("ESPHighlight") 
            if espAtivo and not ex then 
                local hl = Instance.new("Highlight", p.Character) 
                hl.Name = "ESPHighlight" 
                hl.FillTransparency = 0.5 
                hl.FillColor = Color3.fromRGB(255, 0, 0) 
                hl.OutlineColor = Color3.fromRGB(255, 255, 0) 
            elseif not espAtivo and ex then 
                ex:Destroy() 
            end 
        end 
    end 
end)

bn.MouseButton1Click:Connect(function() 
    noclipAtivo = not noclipAtivo 
    bn.Text = noclipAtivo and "üö´ Noclip (Ativado)" or "üö´ Noclip (Desativado)" 
    bn.BackgroundColor3 = noclipAtivo and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(50, 50, 50)
end)

bf.MouseButton1Click:Connect(function()
    toggleFly()
    bf.Text = flyAtivo and "üïäÔ∏è Fly (Ativado)" or "üïäÔ∏è Fly (Desativado)"
    bf.BackgroundColor3 = flyAtivo and Color3.fromRGB(0, 200, 0) or Color3.fromRGB(50, 50, 50)
end)

wsInput.FocusLost:Connect(function(enterPressed) 
    local velocidade = tonumber(wsInput.Text) 
    if velocidade and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then 
        LocalPlayer.Character.Humanoid.WalkSpeed = velocidade 
        notif("Walkspeed Prison Life: " .. velocidade) 
    else 
        notif("N√∫mero inv√°lido! Use 16-500") 
        wsInput.Text = "50" 
    end 
end)

-- PAINEL ELCDX v3.5 Prison Life - PARTE 12 (Fly Controls + Noclip Loop)
RunService.Stepped:Connect(function() 
    if noclipAtivo and LocalPlayer.Character then 
        for _, part in pairs(LocalPlayer.Character:GetDescendants()) do 
            if part:IsA("BasePart") and part ~= LocalPlayer.Character.HumanoidRootPart then 
                part.CanCollide = false 
            end 
        end 
    end
    
    if flyAtivo and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local humanoidRootPart = LocalPlayer.Character.HumanoidRootPart
        local camera = workspace.CurrentCamera
        local moveVector = humanoidRootPart.CFrame:VectorToWorldSpace(
            (UserInputService:IsKeyDown(Enum.KeyCode.D) and Vector3.new(1, 0, 0)) or
            (UserInputService:IsKeyDown(Enum.KeyCode.A) and Vector3.new(-1, 0, 0)) or
            Vector3.new(0, 0, 0)
        )
        
        local camLook = camera.CFrame.LookVector
        local move = Vector3.new(moveVector.X, 0, moveVector.Z).Unit * flySpeed
        
        if UserInputService:IsKeyDown(Enum.KeyCode.W) then
            move = move + camLook * flySpeed
        elseif UserInputService:IsKeyDown(Enum.KeyCode.S) then
            move = move - camLook * flySpeed
        end
        
        if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
            move = move + Vector3.new(0, flySpeed, 0)
        elseif UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
            move = move - Vector3.new(0, flySpeed, 0)
        end
        
        if bodyVelocity then bodyVelocity.Velocity = move end
    end
end)

-- PAINEL ELCDX v3.5 Prison Life - PARTE 13 (Navega√ß√£o + Minimize)
local function mudaPag(n) 
    paginaAtual = n 
    if guiMinimizado then return end 
    for i, p in ipairs(pgs) do 
        p.scrolling.Visible = (i == n) 
    end 
    for i, b in ipairs(btns) do 
        b.TextColor3 = i == n and Color3.fromRGB(0, 255, 0) or Color3.fromRGB(255, 255, 255) 
        b.BackgroundColor3 = i == n and Color3.fromRGB(50, 50, 50) or Color3.fromRGB(35, 35, 35) 
    end 
end

for i, b in ipairs(btns) do 
    b.MouseButton1Click:Connect(function() mudaPag(i) end) 
end

bMin.MouseButton1Click:Connect(function() 
    guiMinimizado = not guiMinimizado 
    if guiMinimizado then 
        for _, p in ipairs(pgs) do p.scrolling.Visible = false end 
        nav.Visible = false 
        f.Size = UDim2.new(0, 220, 0, 40) 
        bMin.Text = "+" 
        t.Text = "ELCDX Prison Life"
    else 
        for i, p in ipairs(pgs) do p.scrolling.Visible = (i == paginaAtual) end 
        nav.Visible = true 
        f.Size = UDim2.new(0, 220, 0, 280) 
        bMin.Text = "-" 
        t.Text = "PAINEL ELCDX v3.5 PRISON"
    end 
end)

-- PAINEL ELCDX v3.5 Prison Life - PARTE 14 (P√°gina 4 - Players + Final)
local function atualizarListaJogadores() 
    pgs[4].container:ClearAllChildren() 
    if not pgs[4].container:FindFirstChild("UIListLayout") then 
        local layout = Instance.new("UIListLayout", pgs[4].container) 
        layout.Padding = UDim.new(0, 3) 
        layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function() 
            pgs[4].container.Size = UDim2.new(1, 0, 0, layout.AbsoluteContentSize.Y) 
            pgs[4].scrolling.CanvasSize = UDim2.new(0, 0, 0, layout.AbsoluteContentSize.Y + 10) 
        end) 
    end 
    for _, player in ipairs(Players:GetPlayers()) do 
        if player ~= LocalPlayer then 
            local frameJogador = Instance.new("Frame", pgs[4].container) 
            frameJogador.Size = UDim2.new(1, -10, 0, 28) 
            frameJogador.BackgroundTransparency = 1 
            
            local botao = Instance.new("TextButton", frameJogador) 
            botao.Size = UDim2.new(1, -60, 1, 0) 
            botao.BackgroundColor3 = Color3.fromRGB(40, 40, 40) 
            botao.TextColor3 = Color3.fromRGB(255, 255, 255) 
            botao.Font = Enum.Font.Code 
            botao.TextSize = 10 
            botao.TextXAlignment = Enum.TextXAlignment.Left 
            local pad = Instance.new("UIPadding", botao) 
            pad.PaddingLeft = UDim.new(0, 8) 
            botao.Text = "üë§ " .. player.Name .. " (@" .. player.DisplayName .. ")" 
            local cornerPlayer = Instance.new("UICorner", botao)
            cornerPlayer.CornerRadius = UDim.new(0, 5)
            
            botao.MouseButton1Click:Connect(function() 
                if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then 
                    tpSimples(player.Character.HumanoidRootPart.Position) 
                else 
                    notif("Personagem de " .. player.Name .. " n√£o encontrado!") 
                end 
            end) 
            
            local botaoSpec = Instance.new("TextButton", frameJogador) 
            botaoSpec.Size = UDim2.new(0, 28, 1, 0) 
            botaoSpec.Position = UDim2.new(1, -56, 0, 0) 
            botaoSpec.BackgroundColor3 = Color3.fromRGB(0, 120, 255) 
            botaoSpec.TextColor3 = Color3.fromRGB(255, 255, 255) 
            botaoSpec.Font = Enum.Font.Code 
            botaoSpec.TextSize = 11 
            botaoSpec.Text = "üëÅÔ∏è" 
            local cornerSpec = Instance.new("UICorner", botaoSpec)
            cornerSpec.CornerRadius = UDim.new(0, 4)
            botaoSpec.MouseButton1Click:Connect(function() iniciarEspectacao(player) end) 
        end 
    end 
end

Players.PlayerAdded:Connect(atualizarListaJogadores) 
Players.PlayerRemoving:Connect(atualizarListaJogadores)
atualizarListaJogadores()

