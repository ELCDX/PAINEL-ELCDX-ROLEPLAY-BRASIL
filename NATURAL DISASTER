MinimizeCorner.CornerRadius = UDim.new(0, 12)
MinimizeCorner.Parent = MinimizeButton

local minimized = false

MinimizeButton.MouseButton1Click:Connect(function()
    minimized = not minimized
    if minimized then
        ContentFrame.Visible = false
        RadiusContainer.Visible = false
        FlySpeedContainer.Visible = false
        FlyStatus.Visible = false
        SuperRingToggle.Size = UDim2.new(1, 0, 0, 70)
        MainFrame.Size = UDim2.new(0, 300, 0, 70)
        MinimizeButton.Text = "+"
        ScrollBar.Visible = false
    else
        ContentFrame.Visible = true
        RadiusContainer.Visible = true
        FlySpeedContainer.Visible = true
        FlyStatus.Visible = true
        SuperRingToggle.Size = UDim2.new(1, 0, 0, 45)
        MainFrame.Size = UDim2.new(0, 300, 0, 350)
        MinimizeButton.Text = "_"
        ScrollBar.Visible = true
        updateScroll()
    end
    playSound("12221967")
end)

CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
    playSound("12221967")
end)

-- Scroll Bar (como no seu script original)
local ScrollBar = Instance.new("Frame")
ScrollBar.Size = UDim2.new(0, 8, 1, -60)
ScrollBar.Position = UDim2.new(1, -10, 0, 50)
ScrollBar.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
ScrollBar.BorderSizePixel = 0
ScrollBar.Parent = MainFrame

local ScrollThumb = Instance.new("Frame")
ScrollThumb.Size = UDim2.new(1, 0, 0.2, 0)
ScrollThumb.Position = UDim2.new(0, 0, 0, 0)
ScrollThumb.BackgroundColor3 = Color3.fromRGB(120, 120, 120)
ScrollThumb.BorderSizePixel = 0
ScrollThumb.Parent = ScrollBar

local UICornerScroll = Instance.new("UICorner")
UICornerScroll.CornerRadius = UDim.new(0, 4)
UICornerScroll.Parent = ScrollBar

local UICornerThumb = Instance.new("UICorner")
UICornerThumb.CornerRadius = UDim.new(0, 4)
UICornerThumb.Parent = ScrollThumb

local scrollPosition = 0
local maxScroll = 0

local function updateScroll()
    local contentSize = ContentFrame.AbsoluteSize.Y
    local containerSize = MainFrame.AbsoluteSize.Y - 80
    
    if contentSize > containerSize then
        maxScroll = contentSize - containerSize
        ScrollBar.Visible = true
        ScrollThumb.Size = UDim2.new(1, 0, containerSize/contentSize, 0)
    else
        ScrollBar.Visible = false
        maxScroll = 0
    end
    
    ContentFrame.Position = UDim2.new(0, 10, 0, 70 - scrollPosition)
    ScrollThumb.Position = UDim2.new(0, 0, scrollPosition/maxScroll, 0)
end

UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseWheel then
        if ScrollBar.Visible then
            scrollPosition = math.clamp(scrollPosition - input.Position.Z * 20, 0, maxScroll)
            updateScroll()
            ScrollThumb.BackgroundColor3 = Color3.fromRGB(160, 160, 160)
            wait(0.3)
            ScrollThumb.BackgroundColor3 = Color3.fromRGB(120, 120, 120)
        end
    end
end)

-- Atualização do sistema de voo com flySpeeds
local flyEnabled = false
local ringPartsEnabled = false
local flyConnection
local tpwalking = false

local function updateFlySystem()
    if flyEnabled and ringPartsEnabled then
        local speaker = LocalPlayer
        local character = speaker.Character
        
        if not character or not character:FindFirstChild("Humanoid") then
            spawn(function()
                wait(1)
                if flyEnabled and ringPartsEnabled then
                    updateFlySystem()
                end
            end)
            return
        end
        
        local humanoid = character.Humanoid
        tpwalking = true
        
        for i = 1, flySpeeds do
            spawn(function()
                local hb = RunService.Heartbeat
                while tpwalking and hb:Wait() and character and humanoid and humanoid.Parent and flyEnabled and ringPartsEnabled do
                    if humanoid.MoveDirection.Magnitude > 0 then
                        character:TranslateBy(humanoid.MoveDirection)
                    end
                end
            end)
        end
        
        if character:FindFirstChild("Animate") then
            character.Animate.Disabled = true
        end
        
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Climbing, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Flying, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Freefall, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.GettingUp, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Landed, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Physics, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.PlatformStanding, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Running, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.RunningNoPhysics, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.StrafingNoPhysics, false)
        humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming, false)
        humanoid:ChangeState(Enum.HumanoidStateType.Swimming)
        
        -- Corpo do voo (exemplo básico, adapte se quiser mais controle)
        local upperTorso = character:FindFirstChild("UpperTorso")
        if upperTorso then
            local bg = Instance.new("BodyGyro", upperTorso)
            bg.P = 90000
            bg.maxTorque = Vector3.new(1e9,1e9,1e9)
            bg.cframe = upperTorso.CFrame
            
            local bv = Instance.new("BodyVelocity", upperTorso)
            bv.velocity = Vector3.new(0,0.1,0)
            bv.maxForce = Vector3.new(1e9,1e9,1e9)
            
            humanoid.PlatformStand = true
            
            getgenv().FlyObjects = {bg, bv}
            
            spawn(function()
                while flyEnabled and ringPartsEnabled and character and humanoid.Health > 0 do
                    RunService.RenderStepped:Wait()
                    -- Você pode adicionar lógica para modificar bv.velocity considerando flySpeeds aqui se quiser mais efeito
                end
                if bg then bg:Destroy() end
                if bv then bv:Destroy() end
            end)
            
            if flyConnection then flyConnection:Disconnect() end
            flyConnection = UserInputService.InputBegan:Connect(function(input)
                -- controle movimento aqui caso queira customizar
            end)
        end
    else
        tpwalking = false
        if flyConnection then flyConnection:Disconnect() flyConnection = nil end
        if getgenv().FlyObjects then
            for _, obj in pairs(getgenv().FlyObjects) do
                if obj and obj.Parent then obj:Destroy() end
            end
            getgenv().FlyObjects = nil
        end
        local character = LocalPlayer.Character
        if character and character:FindFirstChild("Humanoid") then
            character.Humanoid.PlatformStand = false
            if character:FindFirstChild("Animate") then
                character.Animate.Disabled = false
            end
        end
    end
end

-- Evento toggle Super Ring
SuperRingToggle.MouseButton1Click:Connect(function()
    ringPartsEnabled = not ringPartsEnabled
    
    if ringPartsEnabled then
        SuperRingToggle.Text = "SUPER RING: ON"
        SuperRingToggle.BackgroundColor3 = Color3.fromRGB(0, 200, 0)
        
        flyEnabled = true
        FlyStatus.Text = "FLY: ACTIVE"
        FlyStatus.TextColor3 = Color3.fromRGB(100, 255, 100)
        spawn(function() wait(0.1) updateFlySystem() end)
    else
        SuperRingToggle.Text = "SUPER RING: OFF"
        SuperRingToggle.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
        
        flyEnabled = false
        FlyStatus.Text = "FLY: INACTIVE"
        FlyStatus.TextColor3 = Color3.fromRGB(255, 100, 100)
        updateFlySystem()
    end
    playSound("12221967")
end)

updateScroll()
