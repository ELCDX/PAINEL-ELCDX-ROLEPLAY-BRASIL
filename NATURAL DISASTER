-- Controle dos botões de aumentar/diminuir velocidade de voo
local flySpeeds = 1

DecreaseFlySpeed.MouseButton1Click:Connect(function()
    flySpeeds = math.max(1, flySpeeds - 1)
    FlySpeedDisplay.Text = "Fly Speed: " .. flySpeeds
    playSound("12221967")
end)

IncreaseFlySpeed.MouseButton1Click:Connect(function()
    flySpeeds = math.min(10, flySpeeds + 1)
    FlySpeedDisplay.Text = "Fly Speed: " .. flySpeeds
    playSound("12221967")
end)

-- Botões para aumentar/diminuir radius
local radius = 50

DecreaseRadius.MouseButton1Click:Connect(function()
    radius = math.max(10, radius - 5)
    RadiusDisplay.Text = "Radius: " .. radius
    playSound("12221967")
end)

IncreaseRadius.MouseButton1Click:Connect(function()
    radius = math.min(100, radius + 5)
    RadiusDisplay.Text = "Radius: " .. radius
    playSound("12221967")
end)

